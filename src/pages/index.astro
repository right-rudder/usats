---
import BaseLayout from "../layouts/BaseLayout.astro";
import HeaderScreen from "../components/astro/HeaderScreen.astro";
import homePage from "../data/homePage";
import WhyUs from "../components/astro/WhyUs.astro";
import Testimonials from "../components/astro/Testimonials.astro";
import FreQuestions from "../components/astro/FreQuestions.astro";
import "/src/styles/global.css";
import OurAdvantages from "../components/astro/OurAdvantages.astro";
import ElevateYourCarrer from "../components/astro/ElevateYourCarrer.astro";
import LetsFlyTogether from "../components/astro/LetsFlyTogether.astro";
import UnderHeader from "../components/astro/UnderHeader.astro";
import CTADiscoveryFlight from "../components/astro/CTADiscoveryFlights.astro"
import ctaFligth from "../data/ctaFlight";
import Programs from "../components/layouts/ProgramsSwiper.astro";
import programsSwiper from "../data/programsSwiper";
import GridPrograms from "../components/layouts/GridPrograms.astro";

const { data } = homePage;
---

<BaseLayout
  siteTitle={data.pageTitle}
  siteDescription={data.pageDescription}
  keywords={data.pageKeywords}
>
  <!-- HEADER -->
  <HeaderScreen content={data.header} />
  <UnderHeader content={data.underHeader} />
  <!-- <WhoAreUs content={data.whatWeOffer} /> -->
  <WhyUs content={data.whyUs} />
  <!-- <OurAdvantages content={data.ourAdvantages} /> -->
  <!-- <Programs content={programsSwiper} /> -->
  <GridPrograms content={programsSwiper} />
  <ElevateYourCarrer content={data.yourCarrer} />
  <Testimonials content={data.testimonials} />
  <!-- <LetsFlyTogether /> -->

  <FreQuestions content={data.frequentQuestions} />
  <CTADiscoveryFlight content={ctaFligth} />
  <!-- <script src="../scripts/creativeSwiper.js"></script> -->
</BaseLayout>
